<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<VBox spacing="5.0" 
      xmlns="http://javafx.com/javafx/8.0.171" 
      xmlns:fx="http://javafx.com/fxml/1" 
      fx:controller="ifpr.pgua.eic.projetointegrador.controllers.JanelaCadastroCarro">

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label text="Placa"/>
    <TextField fx:id="tfPlaca"/>

    <Label text="Modelo"/>
    <TextField fx:id="tfModelo"/>

    <Label text="Cor"/>
    <TextField fx:id="tfCor"/>

    <Label text="CPF do motorista"/>
    <TextField fx:id="tfCPFMotorista"/>

    <HBox>
        <Button text="Cadastrar" onAction="#cadastrarCarro"/>
    </HBox>

</VBox>