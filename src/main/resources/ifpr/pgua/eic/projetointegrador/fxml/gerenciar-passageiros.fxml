  <rowConstraints>
    <RowConstraints prefHeight="50.0" vgrow="SOMETIMES" />
    <RowConstraints prefHeight="330.0" vgrow="SOMETIMES" />
      <RowConstraints prefHeight="20.0" vgrow="SOMETIMES" />
  </rowConstraints>

    <children>

      <VBox alignment="CENTER" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="1">

        <children>

          <TableView maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="285.0" prefWidth="430.0">

            <columns>
              <TableColumn prefWidth="120.0" text="Nome" />
              <TableColumn prefWidth="75.0" text="Funcao" />
                <TableColumn prefWidth="75.0" text="Curso" />
                <TableColumn prefWidth="90.0" text="Telefone" />
                <TableColumn prefWidth="70.0" text="Idade" />
            </columns>

          </TableView>

          <Button text="Remover" textAlignment="CENTER" />

        </children>

      </VBox>

    </children>

</GridPane>
